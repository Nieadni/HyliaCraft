{
	"schemaVersion": 1,
	"id": "hyliacraft",
	"version": "${version}",
	"name": "HyliaCraft",
	"description": "A Mod which brings content from the various Legend of Zelda games into Minecraft.",
	"authors": [
		"Nieadni",
		"EpicVon2468"
	],
	"contact": {
		"homepage": "https://github.com/Nieadni/HyliaCraft",
		"issues": "https://github.com/Nieadni/HyliaCraft/issues",
		"sources": "https://github.com/Nieadni/HyliaCraft"
	},
	"license": "All-Rights-Reserved",
	"icon": "assets/hyliacraft/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.nieadni.hyliacraft.HyliaCraft"
		],
		"fabric-datagen": [
          "net.nieadni.hyliacraft.data.DataGenerator"
		],
		"client": [
			"net.nieadni.hyliacraft.client.HyliaCraftClient"
		]
	},
	"mixins": [
		"hyliacraft.mixins.json"
	],
	"accessWidener": "hyliacraft.accesswidener",
	"depends": {
		"fabricloader": "*",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*",
		"geckolib": "*",
        "nies-higher-build-limit-512": "*"
